package javaprograms;
import java.util.*;
public class Evennumberrange
{
	public static void main(String args[])
  {
		Scanner in = new Scanner(System.in);
		System.out.print("Enter the number  : ");
		int n = in.nextInt();
		for(int i=2;i<=n;i++)
    {
			if(i%2==0)
				System.out.print(i+" ");
		}
		in.close();
	}
}
