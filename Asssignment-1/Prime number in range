package javaprograms;
import java.util.*;
public class primenumberrange
{
	
	public static void main(String args[])
  {
		Scanner in = new Scanner(System.in);
		System.out.print("Enter the number : ");
		int num = in.nextInt();
		for(int j=2;j<=num;j++)
    {
			int sum=0;
			for(int i=2;i<j;i++)
      {
				if(j%i==0)
        {
					sum=1;
					break;
				}
			}
			if(sum==0)
      {
				System.out.print(j+" ");
			}
		}
		in.close();
	}
}
